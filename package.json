{
  "name": "jest-everywhere",
  "description": "Jest Everywhere presentation",
  "version": "1.0.0",
  "dependencies": {
    "normalize.css": "3.0.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "spectacle": "^1.0.0",
    "spectacle-code-slide": "^0.3.1"
  },
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "eslint": "^1.8.0",
    "eslint-config-defaults": "^7.1.1",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.6.3",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.0",
    "is-buffer": "^1.1.1",
    "jest": "^18.1.0",
    "markdown-loader": "^0.1.7",
    "node-libs-browser": "^0.5.3",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^15.4.2",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "surge": "latest",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.8",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/assets/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\raw!": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "clean": "rimraf dist",
    "deploy": "npm run clean & npm run build && surge -p .",
    "lint": "eslint --ext .js,.jsx .",
    "start": "cross-env NODE_ENV=development node server.js",
    "test": "jest"
  }
}
